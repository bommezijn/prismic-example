                "additional_info": {
                  "type": "StructuredText",
                  "config": {
                    "multi": "paragraph,preformatted,heading3,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,o-list-item",
                    "allowTargetBlank": true,
                    "label": "Additional Info",
